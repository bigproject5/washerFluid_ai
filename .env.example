# Model & Config
MODEL_PATH=models/washer_efficientnet_b0_v1.onnx
CONFIG_PATH=models/washer_config.json
# Fallback threshold if config missing
PREDICT_THRESHOLD=0.5

# Kafka
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC=ai-diagnosis-completed

START_CONSUMER=true

# VehicleAudit Base URL (for inspectionId lookup)
VEHICLE_AUDIT_BASE_URL=http://localhost:8080

# S3
#AWS_ACCESS_KEY_ID=AKIA...
#AWS_SECRET_ACCESS_KEY=...
#AWS_REGION=ap-northeast-2
#S3_BUCKET=your-bucket-name

# S3 / MinIO
AWS_ACCESS_KEY_ID=minio
AWS_SECRET_ACCESS_KEY=minio12345
AWS_REGION=us-east-1
S3_BUCKET=videos
AWS_S3_ENDPOINT=http://localhost:9000
AWS_S3_FORCE_PATH_STYLE=true

# Optional ROI polygon (JSON)
# ROI_POLYGON=[[100,200],[540,200],[540,600],[100,600]